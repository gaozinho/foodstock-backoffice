{
    "name": "Foodstock Backoffice",
    "type": "project",
    "description": "√Årea do cliente",
    "keywords": [
        "Delivery",
        "IFOOD",
        "RAPPI",
        "Gerencie"
    ],
    "license": "SaaS",
    "require": {
        "php": "^7.3|^8.0",
        "bensampo/laravel-enum": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "7.0",
        "jantinnerezo/livewire-alert": "^2.1",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "laravellegends/pt-br-validator": "^8.0",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "proengsoft/laravel-jsvalidation": "^4.4",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-permission": "^4.2",
        "vinkla/hashids": "^9.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "flightsadmin/livewire-crud": "^0.0.14",
        "hannesvdvreken/guzzle-debugbar": "^3.0",
        "krlove/eloquent-model-generator": "^1.3",
        "laravel/sail": "^1.0.1",
        "lucascudo/laravel-pt-br-localization": "^1.1",
        "mockery/mockery": "^1.4.2",
        "nascent-africa/jetstrap": "^2.3",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
